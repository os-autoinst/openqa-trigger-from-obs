<openQA
    project_pattern="openSUSE:(?P&lt;version&gt;Factory):ToTest"
    distri="opensuse">
    <batch name="base" folder="images/local" archs="i586 x86_64">
        <flavor name="Tumbleweed-NET|Tumbleweed-DVD" folder="*product*" iso="1">
            <repos archs="x86_64">
                <oss folder="openSUSE-ftp-ftp" debug="{java*,kernel-default-debug*,kernel-default-base-debug*,mraa-debug*}" source="{coreutils*,yast2-network*}" mirror="1" dest="openSUSE-Tumbleweed-oss-i586-x86_64"/>
                <non-oss folder="openSUSE-Addon-NonOss-ftp-ftp" dest="openSUSE-Tumbleweed-non-oss-i586-x86_64"/>
            </repos>
        </flavor>
        <news iso="Tumbleweed-DVD" archs="x86_64"/>
    </batch>
    <batch name="microos" folder="images/local/*product*" archs="x86_64" repos="base" distri="microos">
        <flavor name="MicroOS-DVD|Kubic-DVD" iso="1" news="1"/>
    </batch>
    <batch name="live" folder="images/*/live*" archs="i686 x86_64" repos="base">
        <flavor name="Rescue-CD|GNOME-Live|KDE-Live|XFCE-Live" iso="1"/>
    </batch>
    <batch name="jeos" folder="appliances/x86_64" archs="x86_64" repos="base">
        <flavor name="JeOS-for-kvm-and-xen" folder="kiwi-templates-JeOS:kvm-and-xen">
            <hdd filemask=".*kvm-and-xen.*\.qcow2$"/>
        </flavor>
    </batch>
    <batch name="microos-qcow" folder="appliances/x86_64" archs="x86_64" repos="base" distri="microos">
        <flavor name="MicroOS-Image-ContainerHost" folder="openSUSE-MicroOS:ContainerHost-kvm-and-xen">
            <hdd filemask=".*ContainerHost-kvm-and-xen.*\.qcow2$"/>
        </flavor>
        <flavor name="MicroOS-Image-Kubic-kubeadm" folder="openSUSE-MicroOS:Kubic-kubeadm-kvm-and-xen">
            <hdd filemask=".*Kubic-kubeadm-kvm-and-xen.*\.qcow2$"/>
        </flavor>
    </batch>
</openQA>
