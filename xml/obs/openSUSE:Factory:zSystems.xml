<openQA
    project_pattern="openSUSE:(?P&lt;version&gt;Factory):zSystems:ToTest"
    distri="opensuse"
    repo_path="images/local"
    media1="0">
    <batch name="base" archs="s390x">
        <flavor name="Tumbleweed-DVD" folder="images/local/*product*" iso="1" archs="s390x">
            <repos>
                <oss folder="0openSUSE" debug="{java*,kernel-default-debug*,kernel-default-base-debug*,mraa-debug*,wicked-debug*}" source="{coreutils*,yast2-network*}" mirror="1" dest="Tumbleweed-oss-s390x"/>
                <non-oss folder="0NonFree" dest="Tumbleweed-non-oss-s390x"/>
            </repos>
        </flavor>
    </batch>
    <batch name="net-agama" folder="appliances/*/agama-installer*" archs="s390x" repos="base">
        <flavor name="agama-installer" iso="1"/>
    </batch>
</openQA>
