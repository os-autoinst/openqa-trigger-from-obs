            <oss folder="0openSUSE" debug="{java*,kernel-default-debug*,kernel-default-base-debug*,mraa-debug*,wicked-debug*}" source="{coreutils*,yast2-network*}" mirror="1" dest="Tumbleweed-oss-$arch"/>
            <non-oss folder="0NonFree" dest="Tumbleweed-non-oss-$arch"/>
        </repos>
    </flavor>
</openQA>
