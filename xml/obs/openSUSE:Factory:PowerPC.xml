    dist_path="obspublish::openqa/openSUSE:Factory:PowerPC:ToTest/images/local"
    distri="opensuse">
    <flavor name="Tumbleweed-NET|Tumbleweed-DVD" folder="*product*" iso="1" archs="ppc64 ppc64le">
